{
    "$schema": "https://turbo.build/schema.json",
    "tasks": {
        "build": {
            "dependsOn": [
                "^build"
            ],
            "outputs": [
                "dist/**"
            ]
        },
        "lint": {},
        "dev": {
            "cache": false
        },
        "start:dev": {
            "cache": false,
            "persistent": true
        },
        "test": {},
        "expo-build": {
            "dependsOn": [
                "^build"
            ],
            "outputs": [
                "apps/mobile/.expo/web/**"
            ]
        }
    }
}